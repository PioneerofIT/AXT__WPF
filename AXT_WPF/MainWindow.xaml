<Window x:Class="AXT_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AXT_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="100" />
        </Grid.RowDefinitions>
        <StackPanel x:Name="">
            
        </StackPanel>
        
        <StackPanel  Grid.Row="1" Orientation="Horizontal">
            <Button Margin="10" Width="100" Content="I/O"></Button>
            <Button Margin="10" Width="100" Content="Motion"></Button>
            <Button Margin="10" Width="100" Content="Exit"></Button>
        </StackPanel>

    </Grid>
</Window>
